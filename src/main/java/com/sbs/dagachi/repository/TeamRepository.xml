<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper	namespace="com.sbs.dagachi.repository.TeamRepository">
	
	<select id="getTeamListByDep" resultType="TeamVO">
		select *
		from Team
		where team_dep=#{dep}
	</select>
	
	<select id="getTeamByCode" resultType="TeamVO">
		select *
		from Team
		where Team_id=#{team}
	</select>

</mapper>